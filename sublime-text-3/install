case "$OSTYPE" in
    darwin*)
        SUBLIME_TEXT_3="$HOME/Library/Application Support/Sublime Text 3"
        ;;
    WSL)
        SUBLIME_TEXT_3="$USERPROFILE/AppData/Roaming/Sublime Text 3"
        ;;
    *)
        SUBLIME_TEXT_3="$HOME/.config/sublime-text-3"
        ;;
esac

link . "$SUBLIME_TEXT_3/Packages/User"
