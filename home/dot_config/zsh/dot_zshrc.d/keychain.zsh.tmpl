{{- $keychain := includeTemplate "find-tool" (dict "root" . "tool" "keychain") }}
{{- if $keychain -}}
# keychain integration (found at: {{ $keychain }})
if [[ -z "$SSH_AUTH_SOCK" ]]; then
{{ output $keychain "--eval" | trim | indent 4 }}
    alias keychain='keychain --absolute --dir "${XDG_RUNTIME_DIR:-$XDG_DATA_HOME}"/keychain'
fi
{{- else }}
{{- end -}}
