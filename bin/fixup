#!/usr/bin/env python

def fixup(fn):
    f = file(fn, 'rU')
    lines = [l.rstrip('\r\n') for l in f.readlines()]
    # lines = f.readlines()
    f.close()

    import types
    if f.newlines and type(f.newlines) != types.StringType:
        print("Processing %s" % fn)
        f = file(fn, 'w')
        f.write('\n'.join(lines))

import sys
for n in range(1,len(sys.argv)):
    fixup(sys.argv[n])
