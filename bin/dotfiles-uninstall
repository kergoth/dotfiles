#!/bin/sh

bindir=$(cd $(dirname $0) && pwd)

$bindir/dotfiles-links | while IFS='	' read linkable dest; do
    if [ -h "$dest" ]; then
        echo >&2 "Removing $dest"
        rm -f "$dest"
    elif [ -e "$dest" ]; then
        echo >&2 "Warning: $dest exists and is not a link, skipping"
        continue
    fi
done
