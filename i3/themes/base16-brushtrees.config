! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #C9DBDC, #C9DBDC, #E3EFEF

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #C9DBDC, #6D828E, #C9DBDC, #C9DBDC, #485867
rofi.color-active:  #C9DBDC, #868cb3, #C9DBDC, #C9DBDC, #868cb3
rofi.color-urgent:  #C9DBDC, #b38686, #C9DBDC, #C9DBDC, #b38686

! Set the desired separator style
rofi.separator-style: solid
