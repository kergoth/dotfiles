# Named directory bits
if [[ -e ~/code ]]; then
    namedir code ~/code
    for i in ~code/*(N/); do
        namedir ${${i:t}//[-_]/} $i
    done
elif [[ -e ~/Code ]]; then
    namedir code ~/Code
    for i in ~code/*(N/); do
        namedir ${${i:t}//[-_]/} $i
    done
fi

case $HOST in
    (*)
        ;;
esac
