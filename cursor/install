link icons.theme ~/.icons/default/index.theme
link gtk-3.0.ini ~/.config/gtk-3.0/settings.ini
link gtkrc-2.0 ~/.gtkrc-2.0

if [ ! -e ~/.icons/Hackneyed ]; then
    (
        cd ~/.icons
        curl -fsSL "https://gitlab.com/Enthymeme/hackneyed-x11-cursors/uploads/bcfe48130c9b898b4516e66d91b7ca6b/Hackneyed-0.6-right-handed.tar.bz2" | tar -jxf -
    )
fi
