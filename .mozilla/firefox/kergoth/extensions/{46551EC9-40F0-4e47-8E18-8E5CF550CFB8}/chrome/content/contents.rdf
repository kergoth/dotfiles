<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

    <RDF:Seq about="urn:mozilla:package:root">
        <RDF:li resource="urn:mozilla:package:stylish"/>
    </RDF:Seq>

    <RDF:Description about="urn:mozilla:package:stylish"
        chrome:name="stylish"
        chrome:displayName="Stylish"
        chrome:author="Jason Barnabe (np)"
        chrome:authorURL="http://userstyles.org/stylish/"
        chrome:description="User Styles"
        chrome:xpcNativeWrappers="no"
        chrome:extension="true"
        chrome:settingsURL="chrome://stylish/content/stylish.xul"
        chrome:iconURL="chrome://stylish/skin/bigicon.png">
    </RDF:Description>

    <RDF:Seq about="urn:mozilla:overlays">
        <RDF:li resource="chrome://browser/content/browser.xul"/>
        <RDF:li resource="chrome://navigator/content/navigator.xul"/>
        <RDF:li resource="chrome://messenger/content/mailWindowOverlay.xul"/>
    </RDF:Seq>

    <RDF:Seq about="chrome://browser/content/browser.xul">
        <RDF:li>chrome://stylish/content/browserOverlay.xul</RDF:li>
    </RDF:Seq>

    <RDF:Seq about="chrome://navigator/content/navigator.xul">
        <RDF:li>chrome://stylish/content/browserOverlay.xul</RDF:li>
    </RDF:Seq>

    <RDF:Seq about="chrome://messenger/content/mailWindowOverlay.xul">
        <RDF:li>chrome://stylish/content/mailOverlay.xul</RDF:li>
    </RDF:Seq>

</RDF:RDF>
