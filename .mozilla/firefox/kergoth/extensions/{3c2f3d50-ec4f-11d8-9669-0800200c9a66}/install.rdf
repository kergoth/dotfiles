<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">

		<em:id>{3c2f3d50-ec4f-11d8-9669-0800200c9a66}</em:id>
		<em:name>del.icio.us Complete</em:name>
		<em:version>1.3</em:version>
		<em:description>del.icio.us is a social bookmarks manager. It allows you to easily add sites you like to your personal collection of links, to categorize those sites with keywords, and to share your collection not only between your own browsers and machines, but also with others.</em:description>
		<em:creator>Christian Hellsten</em:creator>
		<em:contributor>Steven N. Severinghaus</em:contributor>
		<em:contributor>Pasi P. Liimatainen</em:contributor>
		<em:contributor>Daniel Freitag</em:contributor>
        <em:homepageURL>http://delicious.mozdev.org/</em:homepageURL>
		<em:iconURL>chrome://delicious/skin/icon.png</em:iconURL>
		<em:optionsURL>chrome://delicious/content/preferenceDialog.xul</em:optionsURL>

		<em:file>
			<Description about="urn:mozilla:extension:file:delicious.jar">
				<em:package>content/delicious/</em:package>
                <em:locale>locale/en-US/delicious/</em:locale>
                <em:locale>locale/ru-RU/delicious/</em:locale>
                <em:locale>locale/ja/delicious/</em:locale>
                <em:locale>locale/fr-FR/delicious/</em:locale>
                <em:locale>locale/zh-CN/delicious/</em:locale>
				<em:skin>skin/classic/delicious/</em:skin>
			</Description>
		</em:file>

		<em:targetApplication> <!-- Firefox -->
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>1.0</em:minVersion>
				<em:maxVersion>2.0.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>

