if [ ! -e goproj ]; then
    git clone git://github.com/divoxx/goproj
fi
if [ ! -e git-cherry-tree ]; then
    git clone git://github.com/pv/git-cherry-tree
fi
if [ ! -e iln ]; then
    git clone git://github.com/kergoth/iln
fi
if [ ! -e cheat ]; then
    git clone https://github.com/kergoth/cheat
fi
if [ ! -e git-imerge ]; then
    git clone https://github.com/mhagger/git-imerge
fi

link cheat/cheatsheets ~/.cheat

mkdir -p bin
cd bin
ln -sf ../goproj/bin/*
ln -sf ../git-cherry-tree/git-cherry-tree
ln -sf ../iln/iln
ln -sf ../cheat/cheat
ln -sf ../git-imerge/git-imerge
