if [[ $# -eq 0 ]]; then
    local query=
else
    local query="'$*"
fi
local repo="$((z -ldR|sed -ne "s#^\($HOME/Repos/[^/]*/[^/]*/[^/]*\$\)#\1#p" | sed -e "s#^$GIT_PATH/##; s#github.com/##"; gg) | uniq-seen | fzf-tmux --exit-0 ${query:+--select-1 -q "$query"})" || return $?
if [[ -z "$repo" ]]; then
    return 1
fi
gg "$repo"
