}).values();\nexpect(actual.toArray()).toEqual([\"hola\", \"hasta luego\"]);\n\n    });\n\n    spec.suiteId    = 'ObjectLikeSequence-values';\n    spec.exampleId  = 'ObjectLikeSequence-values-1';\n    spec.actual     = 'Lazy({ hello: \\\"hola\\\", goodbye: \\\"hasta luego\\\" }).values()';\n    spec.expected   = 'sequence: [\\\"hola\\\", \\\"hasta luego\\\"]';\n    spec.lineNumber = 0;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"ObjectLikeSequence-watch\">\n                    <h1>\n                        watch\n                        <small>ObjectLikeSequence#watch</small>\n                    </h1>\n\n                    <p>Watches for all changes to a specified property (or properties) of an\nobject and produces a sequence whose elements have the properties\n<code>{ property, value }</code> indicating which property changed and what it was\nchanged to.</p>\n<p>Note that this method <strong>only works on directly wrapped objects</strong>; it will\n<em>not</em> work on any arbitrary <a href=\"#ObjectLikeSequence\">ObjectLikeSequence</a>.</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">ObjectLikeSequence</span>.<span class=\"cm-property\">watch</span> = <span class=\"cm-keyword\">function</span>(<span class=\"cm-def\">propertyNames</span>) { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"ObjectLikeSequence#watch-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n                                <tr>\n                                    <td>propertyNames</td>\n                                    <td>string|Array?</td>\n                                    <td><p>A property name or array of property\n    names to watch. If this parameter is <code>undefined</code>, all of the object&#39;s\n    current (enumerable) properties will be watched.</p>\n</td>\n                                </tr>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>Sequence</td>\n                                    <td><p>A sequence comprising <code>{ property, value }</code> objects\n    describing each change to the specified property/properties.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-ObjectLikeSequence-watch\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">var</span> <span class=\"cm-variable\">obj</span> = {},</span>\n<span class=\"line\" data-line-no=\"1\">    <span class=\"cm-variable\">changes</span> = [];</span>\n<span class=\"line\" data-line-no=\"2\"></span>\n<span class=\"line\" data-line-no=\"3\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">obj</span>).<span class=\"cm-variable\">watch</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-variable\">each</span>(<span class=\"cm-keyword\">function</span>(<span class=\"cm-variable\">change</span>) {</span>\n<span class=\"line\" data-line-no=\"4\">  <span class=\"cm-variable\">changes</span>.<span class=\"cm-variable\">push</span>(<span class=\"cm-variable\">change</span>);</span>\n<span class=\"line\" data-line-no=\"5\">});</span>\n<span class=\"line\" data-line-no=\"6\"></span>\n<span class=\"line\" data-line-no=\"7\"><span class=\"cm-variable\">obj</span>.<span class=\"cm-variable\">foo</span> = <span class=\"cm-number\">1</span>;</span>\n<span class=\"line\" data-line-no=\"8\"><span class=\"cm-variable\">obj</span>.<span class=\"cm-variable\">bar</span> = <span class=\"cm-number\">2</span>;</span>\n<span class=\"line\" data-line-no=\"9\"><span class=\"cm-variable\">obj</span>.<span class=\"cm-variable\">foo</span> = <span class=\"cm-number\">3</span>;</span>\n<span class=\"line\" data-line-no=\"10\"></span>\n<span class=\"line\" data-line-no=\"11\"><span class=\"cm-variable\">obj</span>.<span class=\"cm-variable\">foo</span>; <span class=\"cm-comment\">// =&gt; 3</span></span>\n<span class=\"line\" data-line-no=\"12\"><span class=\"cm-variable\">changes</span>; <span class=\"cm-comment\">// =&gt; [{ property: &#39;foo&#39;, value: 1 }, { property: &#39;foo&#39;, value: 3 }]</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('watch', function() {\n  var obj = {},\n    changes = [];\n\nLazy(obj).watch('foo').each(function(change) {\n  changes.push(change);\n});\n\nobj.foo = 1;\nobj.bar = 2;\nobj.foo = 3;\n  (function() {\n    var spec = it('obj.foo; => 3', function() {\n      var result   = obj.foo;;\n      var expected = 3;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'ObjectLikeSequence-watch';\n    spec.exampleId  = 'ObjectLikeSequence-watch-1';\n    spec.actual     = 'obj.foo;';\n    spec.expected   = '3';\n    spec.lineNumber = 11;\n  }());\n  (function() {\n    var spec = it('changes; => [{ property: \\'foo\\', value: 1 }, { property: \\'foo\\', value: 3 }]', function() {\n      var result   = changes;;\n      var expected = [{ property: 'foo', value: 1 }, { property: 'foo', value: 3 }];\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'ObjectLikeSequence-watch';\n    spec.exampleId  = 'ObjectLikeSequence-watch-2';\n    spec.actual     = 'changes;';\n    spec.expected   = '[{ property: \\'foo\\', value: 1 }, { property: \\'foo\\', value: 3 }]';\n    spec.lineNumber = 12;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"constructor\" id=\"StringLikeSequence\">\n                    <h1>\n                        StringLikeSequence\n                        <small>StringLikeSequence</small>\n                    </h1>\n\n                    <p>A <code>StringLikeSequence</code> represents a sequence of characters.</p>\n<p>The initial sequence you get by wrapping a string with <code>Lazy(string)</code> is a\n<code>StringLikeSequence</code>.</p>\n<p>All methods of <code>StringLikeSequence</code> that conceptually should return\nsomething like a string return another <code>StringLikeSequence</code>.</p>\n\n\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-StringLikeSequence\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">function</span> <span class=\"cm-variable\">upcase</span>(<span class=\"cm-def\">str</span>) { <span class=\"cm-keyword\">return</span> <span class=\"cm-variable-2\">str</span>.<span class=\"cm-property\">toUpperCase</span>(); }</span>\n<span class=\"line\" data-line-no=\"1\"></span>\n<span class=\"line\" data-line-no=\"2\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>)               <span class=\"cm-comment\">// instanceof Lazy.StringLikeSequence</span></span>\n<span class=\"line\" data-line-no=\"3\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-property\">toUpperCase</span>() <span class=\"cm-comment\">// instanceof Lazy.StringLikeSequence</span></span>\n<span class=\"line\" data-line-no=\"4\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-property\">reverse</span>()     <span class=\"cm-comment\">// instanceof Lazy.StringLikeSequence</span></span>\n<span class=\"line\" data-line-no=\"5\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-property\">take</span>(<span class=\"cm-number\">2</span>)       <span class=\"cm-comment\">// instanceof Lazy.StringLikeSequence</span></span>\n<span class=\"line\" data-line-no=\"6\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-property\">drop</span>(<span class=\"cm-number\">1</span>)       <span class=\"cm-comment\">// instanceof Lazy.StringLikeSequence</span></span>\n<span class=\"line\" data-line-no=\"7\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-property\">substring</span>(<span class=\"cm-number\">1</span>)  <span class=\"cm-comment\">// instanceof Lazy.StringLikeSequence</span></span>\n<span class=\"line\" data-line-no=\"8\"></span>\n<span class=\"line\" data-line-no=\"9\"><span class=\"cm-comment\">// Note that `map` does not create a `StringLikeSequence` because there&#39;s</span></span>\n<span class=\"line\" data-line-no=\"10\"><span class=\"cm-comment\">// no guarantee the mapping function will return characters. In the event</span></span>\n<span class=\"line\" data-line-no=\"11\"><span class=\"cm-comment\">// you do want to map a string onto a string-like sequence, use</span></span>\n<span class=\"line\" data-line-no=\"12\"><span class=\"cm-comment\">// `mapString`:</span></span>\n<span class=\"line\" data-line-no=\"13\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-property\">map</span>(<span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">identity</span>)       <span class=\"cm-comment\">// instanceof Lazy.ArrayLikeSequence</span></span>\n<span class=\"line\" data-line-no=\"14\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;foo&#39;</span>).<span class=\"cm-property\">mapString</span>(<span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">identity</span>) <span class=\"cm-comment\">// instanceof Lazy.StringLikeSequence</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('StringLikeSequence', function() {\n  function upcase(str) { return str.toUpperCase(); }\n  (function() {\n    var spec = it('Lazy(\\'foo\\') => instanceof Lazy.StringLikeSequence', function() {\n      var actual = Lazy('foo');\nif (!(actual instanceof Lazy.StringLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\') to be an instance of Lazy.StringLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-1';\n    spec.actual     = 'Lazy(\\'foo\\')';\n    spec.expected   = 'instanceof Lazy.StringLikeSequence';\n    spec.lineNumber = 2;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'foo\\').toUpperCase() => instanceof Lazy.StringLikeSequence', function() {\n      var actual = Lazy('foo').toUpperCase();\nif (!(actual instanceof Lazy.StringLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\').toUpperCase() to be an instance of Lazy.StringLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-2';\n    spec.actual     = 'Lazy(\\'foo\\').toUpperCase()';\n    spec.expected   = 'instanceof Lazy.StringLikeSequence';\n    spec.lineNumber = 3;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'foo\\').reverse() => instanceof Lazy.StringLikeSequence', function() {\n      var actual = Lazy('foo').reverse();\nif (!(actual instanceof Lazy.StringLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\').reverse() to be an instance of Lazy.StringLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-3';\n    spec.actual     = 'Lazy(\\'foo\\').reverse()';\n    spec.expected   = 'instanceof Lazy.StringLikeSequence';\n    spec.lineNumber = 4;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'foo\\').take(2) => instanceof Lazy.StringLikeSequence', function() {\n      var actual = Lazy('foo').take(2);\nif (!(actual instanceof Lazy.StringLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\').take(2) to be an instance of Lazy.StringLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-4';\n    spec.actual     = 'Lazy(\\'foo\\').take(2)';\n    spec.expected   = 'instanceof Lazy.StringLikeSequence';\n    spec.lineNumber = 5;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'foo\\').drop(1) => instanceof Lazy.StringLikeSequence', function() {\n      var actual = Lazy('foo').drop(1);\nif (!(actual instanceof Lazy.StringLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\').drop(1) to be an instance of Lazy.StringLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-5';\n    spec.actual     = 'Lazy(\\'foo\\').drop(1)';\n    spec.expected   = 'instanceof Lazy.StringLikeSequence';\n    spec.lineNumber = 6;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'foo\\').substring(1) => instanceof Lazy.StringLikeSequence', function() {\n      var actual = Lazy('foo').substring(1);\nif (!(actual instanceof Lazy.StringLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\').substring(1) to be an instance of Lazy.StringLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-6';\n    spec.actual     = 'Lazy(\\'foo\\').substring(1)';\n    spec.expected   = 'instanceof Lazy.StringLikeSequence';\n    spec.lineNumber = 7;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'foo\\').map(Lazy.identity) => instanceof Lazy.ArrayLikeSequence', function() {\n      var actual = Lazy('foo').map(Lazy.identity);\nif (!(actual instanceof Lazy.ArrayLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\').map(Lazy.identity) to be an instance of Lazy.ArrayLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-7';\n    spec.actual     = 'Lazy(\\'foo\\').map(Lazy.identity)';\n    spec.expected   = 'instanceof Lazy.ArrayLikeSequence';\n    spec.lineNumber = 13;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'foo\\').mapString(Lazy.identity) => instanceof Lazy.StringLikeSequence', function() {\n      var actual = Lazy('foo').mapString(Lazy.identity);\nif (!(actual instanceof Lazy.StringLikeSequence)) {\n  throw 'Expected Lazy(\\'foo\\').mapString(Lazy.identity) to be an instance of Lazy.StringLikeSequence.';\n}\n    });\n\n    spec.suiteId    = 'StringLikeSequence';\n    spec.exampleId  = 'StringLikeSequence-8';\n    spec.actual     = 'Lazy(\\'foo\\').mapString(Lazy.identity)';\n    spec.expected   = 'instanceof Lazy.StringLikeSequence';\n    spec.lineNumber = 14;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"StringLikeSequence-define\">\n                    <h1>\n                        define\n                        <small>StringLikeSequence.define</small>\n                    </h1>\n\n                    <p>Create a new constructor function for a type inheriting from\n<code>StringLikeSequence</code>.</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">StringLikeSequence</span>.<span class=\"cm-property\">define</span> = <span class=\"cm-keyword\">function</span>(<span class=\"cm-def\">methodName</span>, <span class=\"cm-def\">overrides</span>) { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"StringLikeSequence.defi