d   = '6';\n    spec.lineNumber = 10;\n  }());\n  (function() {\n    var spec = it('Lazy(numbers).find(isNegative) => undefined', function() {\n      var result   = Lazy(numbers).find(isNegative);\n      var expected = undefined;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-find';\n    spec.exampleId  = 'Sequence-find-2';\n    spec.actual     = 'Lazy(numbers).find(isNegative)';\n    spec.expected   = 'undefined';\n    spec.lineNumber = 11;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"Sequence-findWhere\">\n                    <h1>\n                        findWhere\n                        <small>Sequence#findWhere</small>\n                    </h1>\n\n                    <p>Returns the first element in this sequence with property names and values\nmatching those of the specified object.</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">findWhere</span> = <span class=\"cm-keyword\">function</span>(<span class=\"cm-def\">properties</span>) { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#findWhere-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n                                <tr>\n                                    <td>properties</td>\n                                    <td>Object</td>\n                                    <td><p>The properties that should be found on some\n    element in this sequence.</p>\n</td>\n                                </tr>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>*</td>\n                                    <td><p>The found element, or <code>undefined</code> if none exists in this\n    sequence.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-findWhere\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">var</span> <span class=\"cm-variable\">words</span> = [<span class=\"cm-string\">&quot;foo&quot;</span>, <span class=\"cm-string\">&quot;bar&quot;</span>];</span>\n<span class=\"line\" data-line-no=\"1\"></span>\n<span class=\"line\" data-line-no=\"2\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">words</span>).<span class=\"cm-property\">findWhere</span>({ <span class=\"cm-numbercm-property\">0</span>: <span class=\"cm-string\">&quot;f&quot;</span> }); <span class=\"cm-comment\">// =&gt; &quot;foo&quot;</span></span>\n<span class=\"line\" data-line-no=\"3\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">words</span>).<span class=\"cm-property\">findWhere</span>({ <span class=\"cm-numbercm-property\">0</span>: <span class=\"cm-string\">&quot;z&quot;</span> }); <span class=\"cm-comment\">// =&gt; undefined</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('findWhere', function() {\n  var words = [\"foo\", \"bar\"];\n  (function() {\n    var spec = it('Lazy(words).findWhere({ 0: \\\"f\\\" }); => \\\"foo\\\"', function() {\n      var result   = Lazy(words).findWhere({ 0: \"f\" });;\n      var expected = \"foo\";\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-findWhere';\n    spec.exampleId  = 'Sequence-findWhere-1';\n    spec.actual     = 'Lazy(words).findWhere({ 0: \\\"f\\\" });';\n    spec.expected   = '\\\"foo\\\"';\n    spec.lineNumber = 2;\n  }());\n  (function() {\n    var spec = it('Lazy(words).findWhere({ 0: \\\"z\\\" }); => undefined', function() {\n      var result   = Lazy(words).findWhere({ 0: \"z\" });;\n      var expected = undefined;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-findWhere';\n    spec.exampleId  = 'Sequence-findWhere-2';\n    spec.actual     = 'Lazy(words).findWhere({ 0: \\\"z\\\" });';\n    spec.expected   = 'undefined';\n    spec.lineNumber = 3;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"Sequence-first\">\n                    <h1>\n                        first\n                        <small>Sequence#first</small>\n                    </h1>\n\n                    <p>Creates a new sequence comprising the first N elements from this sequence, OR\n(if N is <code>undefined</code>) simply returns the first element of this sequence.</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">first</span> = <span class=\"cm-keyword\">function</span>(<span class=\"cm-def\">count</span>) { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#first-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n                                <tr>\n                                    <td>count</td>\n                                    <td>number?</td>\n                                    <td><p>The number of elements to take from this sequence. If\n    this value exceeds the length of the sequence, the resulting sequence\n    will be essentially the same as this one.</p>\n</td>\n                                </tr>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>*</td>\n                                    <td><p>The new sequence (or the first element from this sequence if\n    no count was given).</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-first\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">function</span> <span class=\"cm-variable\">powerOfTwo</span>(<span class=\"cm-def\">exp</span>) {</span>\n<span class=\"line\" data-line-no=\"1\">  <span class=\"cm-keyword\">return</span> <span class=\"cm-variable\">Math</span>.<span class=\"cm-property\">pow</span>(<span class=\"cm-number\">2</span>, <span class=\"cm-variable-2\">exp</span>);</span>\n<span class=\"line\" data-line-no=\"2\">}</span>\n<span class=\"line\" data-line-no=\"3\"></span>\n<span class=\"line\" data-line-no=\"4\"><span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">generate</span>(<span class=\"cm-variable\">powerOfTwo</span>).<span class=\"cm-property\">first</span>()          <span class=\"cm-comment\">// =&gt; 1</span></span>\n<span class=\"line\" data-line-no=\"5\"><span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">generate</span>(<span class=\"cm-variable\">powerOfTwo</span>).<span class=\"cm-property\">first</span>(<span class=\"cm-number\">5</span>)         <span class=\"cm-comment\">// sequence: [1, 2, 4, 8, 16]</span></span>\n<span class=\"line\" data-line-no=\"6\"><span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">generate</span>(<span class=\"cm-variable\">powerOfTwo</span>).<span class=\"cm-property\">skip</span>(<span class=\"cm-number\">2</span>).<span class=\"cm-property\">first</span>()  <span class=\"cm-comment\">// =&gt; 4</span></span>\n<span class=\"line\" data-line-no=\"7\"><span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">generate</span>(<span class=\"cm-variable\">powerOfTwo</span>).<span class=\"cm-property\">skip</span>(<span class=\"cm-number\">2</span>).<span class=\"cm-property\">first</span>(<span class=\"cm-number\">2</span>) <span class=\"cm-comment\">// sequence: [4, 8]</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('first', function() {\n  function powerOfTwo(exp) {\n  return Math.pow(2, exp);\n}\n  (function() {\n    var spec = it('Lazy.generate(powerOfTwo).first() => 1', function() {\n      var result   = Lazy.generate(powerOfTwo).first();\n      var expected = 1;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-first';\n    spec.exampleId  = 'Sequence-first-1';\n    spec.actual     = 'Lazy.generate(powerOfTwo).first()';\n    spec.expected   = '1';\n    spec.lineNumber = 4;\n  }());\n  (function() {\n    var spec = it('Lazy.generate(powerOfTwo).first(5) => sequence: [1, 2, 4, 8, 16]', function() {\n      var actual = Lazy.generate(powerOfTwo).first(5);\nexpect(actual.toArray()).toEqual([1, 2, 4, 8, 16]);\n\n    });\n\n    spec.suiteId    = 'Sequence-first';\n    spec.exampleId  = 'Sequence-first-2';\n    spec.actual     = '