                 <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>Sequence</td>\n                                    <td><p>The new sequence.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-flatten\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">1</span>, [<span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>], [<span class=\"cm-number\">4</span>, [<span class=\"cm-number\">5</span>]]]).<span class=\"cm-property\">flatten</span>() <span class=\"cm-comment\">// sequence: [1, 2, 3, 4, 5]</span></span>\n<span class=\"line\" data-line-no=\"1\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">1</span>, <span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>])]).<span class=\"cm-property\">flatten</span>()     <span class=\"cm-comment\">// sequence: [1, 2, 3]</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('flatten', function() {\n  \n  (function() {\n    var spec = it('Lazy([1, [2, 3], [4, [5]]]).flatten() => sequence: [1, 2, 3, 4, 5]', function() {\n      var actual = Lazy([1, [2, 3], [4, [5]]]).flatten();\nexpect(actual.toArray()).toEqual([1, 2, 3