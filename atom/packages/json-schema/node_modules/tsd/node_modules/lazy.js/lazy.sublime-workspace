));\n  (function() {\n    var spec = it('Lazy([1, 2, 3]).chunk(4) => sequence: [[1, 2, 3]]', function() {\n      var actual = Lazy([1, 2, 3]).chunk(4);\nexpect(actual.toArray()).toEqual([[1, 2, 3]]);\n\n    });\n\n    spec.suiteId    = 'Sequence-chunk';\n    spec.exampleId  = 'Sequence-chunk-4';\n    spec.actual     = 'Lazy([1, 2, 3]).chunk(4)';\n    spec.expected   = 'sequence: [[1, 2, 3]]';\n    spec.lineNumber = 3;\n  }());\n  (function() {\n    var spec = it('Lazy([1, 2, 3]).chunk(0) => throws', function() {\n      expect(function() { Lazy([1, 2, 3]).chunk(0) }).toThrow();\n    });\n\n    spec.suiteId    = 'Sequence-chunk';\n    spec.exampleId  = 'Sequence-chunk-5';\n    spec.actual     = 'Lazy([1, 2, 3]).chunk(0)';\n    spec.expected   = 'throws';\n    spec.lineNumber = 4;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"Sequence-compact\">\n                    <h1>\n                        compact\n                        <small>Sequence#compact</small>\n                    </h1>\n\n                    <p>Creates a new sequence with the same elements as this one, except for all\nfalsy values (<code>false</code>, <code>0</code>, <code>&quot;&quot;</code>, <code>null</code>, and <code>undefined</code>).</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">compact</span> = <span class=\"cm-keyword\">function</span>() { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#compact-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>Sequence</td>\n                                    <td><p>The new sequence.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-compact\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-string\">&quot;foo&quot;</span>, <span class=\"cm-atom\">null</span>, <span class=\"cm-string\">&quot;bar&quot;</span>, <span class=\"cm-atom\">undefined</span>]).<span class=\"cm-property\">compact</span>() <span class=\"cm-comment\">// sequence: [&quot;foo&quot;, &quot;bar&quot;]</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('compact', function() {\n  \n  (function() {\n    var spec = it('Lazy([\\\"foo\\\", null, \\\"bar\\\", undefined]).compact() => sequence: [\\\"foo\\\", \\\"bar\\\"]', function() {\n      var actual = Lazy([\"foo\", null, \"bar\", undefined]).compact();\nexpect(actual.toArray()).toEqual([\"foo\", \"bar\"]);\n\n    });\n\n    spec.suiteId    = 'Sequence-compact';\n    spec.exampleId  = 'Sequence-compact-1';\n    spec.actual     = 'Lazy([\\\"foo\\\", null, \\\"bar\\\", undefined]).compact()';\n    spec.expected   = 'sequence: [\\\"foo\\\", \\\"bar\\\"]';\n    spec.lineNumber = 0;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"Sequence-concat\">\n                    <h1>\n                        concat\n                        <small>Sequence#concat</small>\n                    </h1>\n\n                    <p>Creates a new sequence with all of the elements of this one, plus those of\nthe given array(s).</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">concat</span> = <span class=\"cm-keyword\">function</span>(<span class=\"cm-def\">var_args</span>) { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#concat-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n                                <tr>\n                                    <td>var_args</td>\n                                    <td>...*</td>\n                                    <td><p>One or more values (or arrays of values) to use for\n    additional items after this sequence.</p>\n</td>\n                                </tr>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>Sequence</td>\n                                    <td><p>The new sequence.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-concat\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">var</span> <span class=\"cm-variable\">left</span>  = [<span class=\"cm-number\">1</span>, <span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>];</span>\n<span class=\"line\" data-line-no=\"1\"><span class=\"cm-keyword\">var</span> <span class=\"cm-variable\">right</span> = [<span class=\"cm-number\">4</span>, <span class=\"cm-number\">5</span>, <span class=\"cm-number\">6</span>];</span>\n<span class=\"line\" data-line-no=\"2\"></span>\n<span class=\"line\" data-line-no=\"3\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">left</span>).<span class=\"cm-property\">concat</span>(<span class=\"cm-variable\">right</span>)         <span class=\"cm-comment\">// sequence: [1, 2, 3, 4, 5, 6]</span></span>\n<span class=\"line\" data-line-no=\"4\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">left</span>).<span class=\"cm-property\">concat</span>(<span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">right</span>))   <span class=\"cm-comment\">// sequence: [1, 2, 3, 4, 5, 6]</span></span>\n<span class=\"line\" data-line-no=\"5\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">left</span>).<span class=\"cm-property\">concat</span>(<span class=\"cm-variable\">right</span>, [<span class=\"cm-number\">7</span>, <span class=\"cm-number\">8</span>]) <span class=\"cm-comment\">// sequence: [1, 2, 3, 4, 5, 6, 7, 8]</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('concat', function() {\n  var left  = [1, 2, 3];\nvar right = [4, 5, 6];\n  (function() {\n    var spec = it('Lazy(left).concat(right) => sequence: [1, 2, 3, 4, 5, 6]', function() {\n      var actual = Lazy(left).concat(right);\nexpect(actual.toArray()).toEqual([1, 2, 3, 4, 5, 6]);\n\n    });\n\n    spec.suiteId    = 'Sequence-concat';\n    spec.exampleId  = 'Sequence-concat-1';\n    spec.actual     = 'Lazy(left).concat(right)';\n    s