            <section class=\"method\" id=\"Sequence-shuffle\">\n                    <h1>\n                        shuffle\n                        <small>Sequence#shuffle</small>\n                    </h1>\n\n                    <p>Creates a new sequence with the same elements as this one, in a randomized\norder.</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">shuffle</span> = <span class=\"cm-keyword\">function</span>() { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#shuffle-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>Sequence</td>\n                                    <td><p>The new sequence.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-shuffle\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">1</span>, <span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>, <span class=\"cm-number\">4</span>, <span class=\"cm-number\">5</span>]).<span class=\"cm-property\">shuffle</span>() <span class=\"cm-comment\">// the values [1, 2, 3, 4, 5] in any order</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('shuffle', function() {\n  \n  (function() {\n    var spec = it('Lazy([1, 2, 3, 4, 5]).shuffle() => the values [1, 2, 3, 4, 5] in any order', function() {\n      var actual = Lazy([1, 2, 3, 4, 5]).shuffle();\n\nactual = actual.toArray().sort();\n\nexpect(actual).toEqual([1, 2, 3, 4, 5].sort());\n\n    });\n\n    spec.suiteId    = 'Sequence-shuffle';\n    spec.exampleId  = 'Sequence-shuffle-1';\n    spec.actual     = 'Lazy([1, 2, 3, 4, 5]).shuffle()';\n    spec.expected   = 'the values [1, 2, 3, 4, 5] in any order';\n    spec.lineNumber = 0;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"Sequence-size\">\n                    <h1>\n                        size\n                        <small>Sequence#size</small>\n                    </h1>\n\n                    <p>Gets the number of elements in the sequence. In some cases, this may\nrequire eagerly evaluating the sequence.</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">size</span> = <span class=\"cm-keyword\">function</span>() { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#size-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>number</td>\n                                    <td><p>The number of elements in the sequence.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-size\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">function</span> <span class=\"cm-variable\">isEven</span>(<span class=\"cm-def\">x</span>) { <span class=\"cm-keyword\">return</span> <span class=\"cm-variable-2\">x</span> % <span class=\"cm-number\">2</span> === <span class=\"cm-number\">0</span>; }</span>\n<span class=\"line\" data-line-no=\"1\"></span>\n<span class=\"line\" data-line-no=\"2\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">1</span>, <span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>]).<span class=\"cm-property\">size</span>();                 <span class=\"cm-comment\">// =&gt; 3</span></span>\n<span class=\"line\" data-line-no=\"3\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">1</span>, <span class=\"cm-number\">2</span>]).<span class=\"cm-property\">map</span>(<span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">identity</span>).<span class=\"cm-property\">size</span>(); <span class=\"cm-comment\">// =&gt; 2</span></span>\n<span class=\"line\" data-line-no=\"4\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">1</span>, <span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>]).<span class=\"cm-property\">reject</span>(<span class=\"cm-variable\">isEven</span>).<span class=\"cm-property\">size</span>();  <span class=\"cm-comment\">// =&gt; 2</span></span>\n<span class=\"line\" data-line-no=\"5\"><span class=\"cm-variable\">Lazy</span>([<span class=\"cm-number\">1</span>, <span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>]).<span class=\"cm-property\">take</span>(<span class=\"cm-number\">1</span>).<span class=\"cm-property\">size</span>();         <span class=\"cm-comment\">// =&gt; 1</span></span>\n<span class=\"line\" data-line-no=\"6\"><span class=\"cm-variable\">Lazy</span>({ <span class=\"cm-property\">foo</span>: <span class=\"cm-number\">1</span>, <span class=\"cm-property\">bar</span>: <span class=\"cm-number\">2</span> }).<span class=\"cm-property\">size</span>();        <span class=\"cm-comment\">// =&gt; 2</span></span>\n<span class=\"line\" data-line-no=\"7\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-string\">&#39;hello&#39;</span>).<span class=\"cm-property\">size</span>();                   <span class=\"cm-comment\">// =&gt; 5</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('size', function() {\n  function isEven(x) { return x % 2 === 0; }\n  (function() {\n    var spec = it('Lazy([1, 2, 3]).size(); => 3', function() {\n      var result   = Lazy([1, 2, 3]).size();;\n      var expected = 3;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-size';\n    spec.exampleId  = 'Sequence-size-1';\n    spec.actual     = 'Lazy([1, 2, 3]).size();';\n    spec.expected   = '3';\n    spec.lineNumber = 2;\n  }());\n  (function() {\n    var spec = it('Lazy([1, 2]).map(Lazy.identity).size(); => 2', function() {\n      var result   = Lazy([1, 2]).map(Lazy.identity).size();;\n      var expected = 2;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-size';\n    spec.exampleId  = 'Sequence-size-2';\n    spec.actual     = 'Lazy([1, 2]).map(Lazy.identity).size();';\n    spec.expected   = '2';\n    spec.lineNumber = 3;\n  }());\n  (function() {\n    var spec = it('Lazy([1, 2, 3]).reject(isEven).size(); => 2', function() {\n      var result   = Lazy([1, 2, 3]).reject(isEven).size();;\n      var expected = 2;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-size';\n    spec.exampleId  = 'Sequence-size-3';\n    spec.actual     = 'Lazy([1, 2, 3]).reject(isEven).size();';\n    spec.expected   = '2';\n    spec.lineNumber = 4;\n  }());\n  (function() {\n    var spec = it('Lazy([1, 2, 3]).take(1).size(); => 1', function() {\n      var result   = Lazy([1, 2, 3]).take(1).size();;\n      var expected = 1;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-size';\n    spec.exampleId  = 'Sequence-size-4';\n    spec.actual     = 'Lazy([1, 2, 3]).take(1).size();';\n    spec.expected   = '1';\n    spec.lineNumber = 5;\n  }());\n  (function() {\n    var spec = it('Lazy({ foo: 1, bar: 2 }).size(); => 2', function() {\n      var result   = Lazy({ foo: 1, bar: 2 }).size();;\n      var expected = 2;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-size';\n    spec.exampleId  = 'Sequence-size-5';\n    spec.actual     = 'Lazy({ foo: 1, bar: 2 }).size();';\n    spec.expected   = '2';\n    spec.lineNumber = 6;\n  }());\n  (function() {\n    var spec = it('Lazy(\\'hello\\').size(); => 5', function() {\n      var result   = Lazy('hello').size();;\n      var expected = 5;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-size';\n    spec.exampleId  = 'Sequence-size-6';\n    spec.actual     = 'Lazy(\\'hello\\').size();';\n    spec.expected   = '5';\n    spec.lineNumber = 7;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"Sequence-some\">\n                    <h1>\n                        some\n                        <small>Sequence#some</small>\n                    </h1>\n\n                    <p>Checks whether at least one element in this sequence satisfies a given\npredicate (or, if no predicate is specified, whether the sequence contains at\nleast one element).</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">some</span> = <span class=\"cm-keyword\">function</span>(<span class=\"cm-def\">predicate</span>) { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#some-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n                                <tr>\n                                    <td>predicate</td>\n                                    <td>Function?</td>\n                                    <td><p>A function to call on (potentially) every element\n    in this sequence.</p>\n</td>\n                                </tr>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>boolean</td>\n                                    <td><p>True if <code>predicate</code> returns true for at least one element\n    in the sequence. False if <code>predicate</code> returns false for every element (or\n    the sequence is empty).</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-some\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">function</span> <span class=\"cm-variable\">isEven</span>(<span class=\"cm-def\">x</span>) { <span class=\"cm-keyword\">return</span> <span class=\"cm-variable-2\">x</span> % <span class=\"cm-number\">2</span> === <span class=\"cm-number\">0</span>; }</span>\n<span class=\"line\" data-line-no=\"1\"><span class=\"cm-keyword\">function</span> <span class=\"cm-variable\">isNegative</span>(<span class=\"cm-def\">x</span>) { <span class=\"cm-keyword\">return</span> <span class=\"cm-variable-2\">x</span> &lt; <span class=\"cm-number\">0</span>; }</span>\n<span class=\"line\" data-line-no=\"2\"></span>\n<span class=\"line\" data-line-no=\"3\"><span class=\"cm-keyword\">var</span> <span class=\"cm-variable\">numbers</span> = [<span class=\"cm-number\">1</span>, <span class=\"cm-number\">2</span>, <span class=\"cm-number\">3</span>, <span class=\"cm-number\">4</span>, <span class=\"cm-number\">5</span>];</span>\n<span class=\"line\" data-line-no=\"4\"></span>\n<span class=\"line\" data-line-no=\"5\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">numbers</span>).<span class=\"cm-property\">some</span>()           <span class=\"cm-comment\">// =&gt; true</span></span>\n<span class=\"line\" data-line-no=\"6\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">numbers</span>).<span class=\"cm-property\">some</span>(<span class=\"cm-variable\">isEven</span>)     <span class=\"cm-comment\">// =&gt; true</span></span>\n<span class=\"line\" data-line-no=\"7\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">numbers</span>).<span class=\"cm-property\">some</span>(<span class=\"cm-variable\">isNegative</span>) <span class=\"cm-comment\">// =&gt; false</span></span>\n<span class=\"line\" data-line-no=\"8\"><span class=\"cm-variable\">Lazy</span>([]).<span class=\"cm-variable\">some</span>()                <span class=\"cm-comment\">// =&gt; false</span></span></pre>\n                    </div>\n\n\n                    <script type=\"text/javascript\">\ndescribe('some', function() {\n  function isEven(x) { return x % 2 === 0; }\nfunction isNegative(x) { return x < 0; }\n\nvar numbers = [1, 2, 3, 4, 5];\n  (function() {\n    var spec = it('Lazy(numbers).some() => true', function() {\n      var result   = Lazy(numbers).some();\n      var expected = true;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-some';\n    spec.exampleId  = 'Sequence-some-1';\n    spec.actual     = 'Lazy(numbers).some()';\n    spec.expected   = 'true';\n    spec.lineNumber = 5;\n  }());\n  (function() {\n    var spec = it('Lazy(numbers).some(isEven) => true', function() {\n      var result   = Lazy(numbers).some(isEven);\n      var expected = true;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-some';\n    spec.exampleId  = 'Sequence-some-2';\n    spec.actual     = 'Lazy(numbers).some(isEven)';\n    spec.expected   = 'true';\n    spec.lineNumber = 6;\n  }());\n  (function() {\n    var spec = it('Lazy(numbers).some(isNegative) => false', function() {\n      var result   = Lazy(numbers).some(isNegative);\n      var expected = false;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-some';\n    spec.exampleId  = 'Sequence-some-3';\n    spec.actual     = 'Lazy(numbers).some(isNegative)';\n    spec.expected   = 'false';\n    spec.lineNumber = 7;\n  }());\n  (function() {\n    var spec = it('Lazy([]).some() => false', function() {\n      var result   = Lazy([]).some();\n      var expected = false;\n      assertEquality(expected, result);\n    });\n\n    spec.suiteId    = 'Sequence-some';\n    spec.exampleId  = 'Sequence-some-4';\n    spec.actual     = 'Lazy([]).some()';\n    spec.expected   = 'false';\n    spec.lineNumber = 8;\n  }());\n});\n                    </script>\n                </section>\n                <section class=\"method\" id=\"Sequence-sortBy\">\n                    <h1>\n                        sortBy\n                        <small>Sequence#sortBy</small>\n                    </h1>\n\n                    <p>Creates a new sequence with the same elements as this one, but ordered\naccording to the values returned by the specified function.</p>\n\n\n                    <div class=\"signature\">\n                        <h3>Signature</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-variable\">Sequence</span>.<span class=\"cm-property\">sortBy</span> = <span class=\"cm-keyword\">function</span>(<span class=\"cm-def\">sortFn</span>) { <span class=\"cm-comment\">/*...*/</span> }</span></pre>\n\n                        <table id=\"Sequence#sortBy-parameters\">\n                            <thead>\n                                <tr>\n                                    <th>Name</th>\n                                    <th>Type(s)</th>\n                                    <th>Description</th>\n                                </tr>\n                            </thead>\n\n                            <tbody>\n                                <tr>\n                                    <td>sortFn</td>\n                                    <td>Function</td>\n                                    <td><p>The function to call on the elements in this\n    sequence, in order to sort them.</p>\n</td>\n                                </tr>\n\n                            </tbody>\n\n                            <tfoot>\n                                <tr>\n                                    <td>returns</td>\n                                    <td>Sequence</td>\n                                    <td><p>The new sequence.</p>\n</td>\n                                </tr>\n                            </tfoot>\n                        </table>\n                    </div>\n\n                    <div class=\"examples\">\n                        <h3>Examples</h3>\n\n                        <pre class=\"source-examples\" id=\"source-Sequence-sortBy\"><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">function</span> <span class=\"cm-variable\">population</span>(<span class=\"cm-def\">country</span>) {</span>\n<span class=\"line\" data-line-no=\"1\">  <span class=\"cm-keyword\">return</span> <span class=\"cm-variable-2\">country</span>.<span class=\"cm-property\">pop</span>;</span>\n<span class=\"line\" data-line-no=\"2\">}</span>\n<span class=\"line\" data-line-no=\"3\"></span>\n<span class=\"line\" data-line-no=\"4\"><span class=\"cm-keyword\">function</span> <span class=\"cm-variable\">area</span>(<span class=\"cm-def\">country</span>) {</span>\n<span class=\"line\" data-line-no=\"5\">  <span class=\"cm-keyword\">return</span> <span class=\"cm-variable-2\">country</span>.<span class=\"cm-property\">sqkm</span>;</span>\n<span class=\"line\" data-line-no=\"6\">}</span>\n<span class=\"line\" data-line-no=\"7\"></span>\n<span class=\"line\" data-line-no=\"8\"><span class=\"cm-keyword\">var</span> <span class=\"cm-variable\">countries</span> = [</span>\n<span class=\"line\" data-line-no=\"9\">  { <span class=\"cm-property\">name</span>: <span class=\"cm-string\">&quot;USA&quot;</span>, <span class=\"cm-property\">pop</span>: <span class=\"cm-number\">320000000</span>, <span class=\"cm-property\">sqkm</span>: <span class=\"cm-number\">9600000</span> },</span>\n<span class=\"line\" data-line-no=\"10\">  { <span class=\"cm-property\">name</span>: <span class=\"cm-string\">&quot;Brazil&quot;</span>, <span class=\"cm-property\">pop</span>: <span class=\"cm-number\">194000000</span>, <span class=\"cm-property\">sqkm</span>: <span class=\"cm-number\">8500000</span> },</span>\n<span class=\"line\" data-line-no=\"11\">  { <span class=\"cm-property\">name</span>: <span class=\"cm-string\">&quot;Nigeria&quot;</span>, <span class=\"cm-property\">pop</span>: <span class=\"cm-number\">174000000</span>, <span class=\"cm-property\">sqkm</span>: <span class=\"cm-number\">924000</span> },</span>\n<span class=\"line\" data-line-no=\"12\">  { <span class=\"cm-property\">name</span>: <span class=\"cm-string\">&quot;China&quot;</span>, <span class=\"cm-property\">pop</span>: <span class=\"cm-number\">1350000000</span>, <span class=\"cm-property\">sqkm</span>: <span class=\"cm-number\">9700000</span> },</span>\n<span class=\"line\" data-line-no=\"13\">  { <span class=\"cm-property\">name</span>: <span class=\"cm-string\">&quot;Russia&quot;</span>, <span class=\"cm-property\">pop</span>: <span class=\"cm-number\">143000000</span>, <span class=\"cm-property\">sqkm</span>: <span class=\"cm-number\">17000000</span> },</span>\n<span class=\"line\" data-line-no=\"14\">  { <span class=\"cm-property\">name</span>: <span class=\"cm-string\">&quot;Australia&quot;</span>, <span class=\"cm-property\">pop</span>: <span class=\"cm-number\">23000000</span>, <span class=\"cm-property\">sqkm</span>: <span class=\"cm-number\">7700000</span> }</span>\n<span class=\"line\" data-line-no=\"15\">];</span>\n<span class=\"line\" data-line-no=\"16\"></span>\n<span class=\"line\" data-line-no=\"17\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">countries</span>).<span class=\"cm-property\">sortBy</span>(<span class=\"cm-variable\">population</span>).<span class=\"cm-property\">last</span>(<span class=\"cm-number\">3</span>).<span class=\"cm-property\">pluck</span>(<span class=\"cm-string\">&#39;name&#39;</span>) <span class=\"cm-comment\">// sequence: [&quot;Brazil&quot;, &quot;USA&quot;, &quot;China&quot;]</span></span>\n<span class=\"line\" data-line-no=\"18\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">countries</span>).<span class=\"cm-property\">sortBy</span>(<span class=\"cm-variable\">area</span>).<span class=\"cm-property\">last</span>(<span class=\"cm-number\">3</span>).<span class=\"cm-property\">pluck</span>(<span class=\"cm-string\">&#39;name&#39;</span>)       <span class=\"cm-comment\">// sequence: [&quot;USA&quot;, &quot;China&quot;, &quot;Russia&quot;]</span></span></pre>\n                    </div>\n\n                    <div class=\"perf\">\n                        <h3>Benchmarks</h3>\n\n                        <pre><span class=\"line\" data-line-no=\"0\"><span class=\"cm-keyword\">var</span> <span class=\"cm-variable\">randoms</span> = <span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">generate</span>(<span class=\"cm-variable\">Math</span>.<span class=\"cm-property\">random</span>).<span class=\"cm-property\">take</span>(<span class=\"cm-number\">100</span>).<span class=\"cm-property\">toArray</span>();</span>\n<span class=\"line\" data-line-no=\"1\"></span>\n<span class=\"line\" data-line-no=\"2\"><span class=\"cm-variable\">Lazy</span>(<span class=\"cm-variable\">randoms</span>).<span class=\"cm-property\">sortBy</span>(<span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">identity</span>).<span class=\"cm-property\">each</span>(<span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">noop</span>) <span class=\"cm-comment\">// lazy</span></span>\n<span class=\"line\" data-line-no=\"3\"><span class=\"cm-variable\">_</span>.<span class=\"cm-property\">each</span>(<span class=\"cm-variable\">_</span>.<span class=\"cm-property\">sortBy</span>(<span class=\"cm-variable\">randoms</span>, <span class=\"cm-variable\">Lazy</span>.<span class=\"cm-property\">identity</span>), <span class=\"cm-variable\">_</span>.<span class=\"cm-property\">noop</span>)    <span class=\"cm-comment\">// lodash</span></span></pre>\n\n                        <table id=\"Sequence-sortBy-perf-tests\">\n                            <tr>\n                                <th>Implementation</th>\n                                <th>Ops&#x2F;second</th>\n                            </tr>\n\n                            <tr id=\"perf-test-Sequence-sortBy-1\">\n                                <td>lazy</td>\n                                <td data-case-id=\"1\"></td>\n                            </tr>\n                            <tr id=\"perf-test-Sequence-sortBy-2\">\n                                <td>lodash</td>\n                                <td data-case-id=\"2\"></td>\n                            </tr>\n                        </table>\n\n                        <button>Run performance benchmarks</button>\n                    </div>\n\n                    <script type=\"text/javascript\">\ndescribe('sortBy', function() {\n  function population(country) {\n  return country.pop;\n}\n\nfunction area(country) {\n  return country.sqkm;\n}\n\nvar countries = [\n  { name: \"USA\", pop: 320000000, sqkm: 9600000 },\n  { name: \"Brazil\", pop: 194000000, sqkm: 8500000 },\n  { name: \"Nigeria\", pop: 174000000, sqkm: 924000 },\n  { name: \"China\", pop: 1350000000, sqkm: 9700000 },\n  { name: \"Russia\", pop: 143000000, sqkm: 17000000 },\n  { name: \"Australia\", pop: 23000000, sqkm: 7700000 }\n];\n  (function() {\n    var spec = it('Lazy(countries).sortBy(population).last(3).pluck(\\'name\\') => sequence: [\\\"Brazil\\\", \\\"USA\\\", \\\"China\\\"]', function() {\n      var actual = Lazy(countries).sortBy(population).last(3).pluck('name');\nexpect(actual.toArray()).toEqual([\"Brazil\", \"USA\", \"China\"]);\n\n    });\n\n    spec.suiteId    = 'Sequence-sortBy';\n    spec.exampleId  = 'Sequence-sortBy-1';\n    spec.actual     = 'Lazy(countries).sortBy(population).last(3).pluck(\\'name\\')';\n    spec.expected   = 'sequence: [\\\"Brazil\\\", \\\"USA\\\", \\\"China\\\"]';\n    spec.lineNumber = 17;\n  }());\n  (function() {\n    var spec = it('Lazy(countries).sortBy(area).last(3).pluck(\\'name\\') => sequence: [\\\"USA\\\", \\\"China\\\", \\\"Russia\\\"]', function() {\n      var actual = Lazy(countries).sortBy(area).last(3).pluck('name');\nexpect(actual.toArray()).toEqual([\"USA\", \"China\", \"Russia\"]);\n\n    });\n\n    s